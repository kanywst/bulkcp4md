{
    "name": "bulkcp4md",
    "description": "bulkcp4md is bulk copy of title and URL for markdown",
    "version": "0.0.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"],
    "action": {},
    "background": {
		"service_worker": "main.js"
	},
    "commands": {
        "_execute_action": {
        "suggested_key": {
            "default": "Ctrl+Shift+H",
            "mac": "MacCtrl+Shift+H"
        },
        "description": "Bulk copy of title and URL."
        }
    }
}
